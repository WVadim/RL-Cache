{
	"cache size": 1024,
	"data folder": "data/china_featured",
	
	"lambda features": 0.99,

	"batch size": 4096,

	"statistics":
		{
			"statistics": "auxiliary/features_primary.npy",
			"warmup": 1000000,
			"split step": 1
		},

	"model":
		{
			"wing size": 3,
			
			"dropout rate": 0.0,
			"use common": false,
			"multiplier common": 20,
			"layers common": 3,

			"multiplier each": 100,
			"layers each": 3,

			"use batch normalization": true,

			"eviction lr": 1e-5,
			"admission lr": 1e-5

		},
	"testing":
		{
		"algorithms": ["AL-GDSF", "AL-LRU", "AL-Oracle", "ML-ML-RNG", "ML-ML-DET"],
		"period": 360,
		"requests max": 200000000,
		"reset": true,
		"alpha": 0
		},
	"training":
		{
			"target": "ML-GDSF",

			"session configuration":
					{
						"collect discounted": false,
						"randomness change": false,
						"point of change": 0,
						"gamma": 0.01,
						"alpha": 0
					},

			"requests max": 10000000,
			"samples": 450,

			"percentile admission": 95,
			"percentile eviction": 95,

			"epochs": 1,
			"warmup": 500000,
			"runs": 10,
			"repetitions": 1,
			"drop": true,
			
			"overlap": 0,
			"period": 300000,

			"refresh policy": "monotonic",
			"refresh value": 10,

			"algorithms": ["AL-GDSF", "SH-GDSF"],

			"iterative": false,
			"start iteration": "E",

			"IP:train admission": true,
			"IP:train eviction": true,

			"dump sessions": true,
			"dump limit": 200,
			"dump percentile": 75
		}

}
