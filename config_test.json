{
	"cache size": 1024,
	"data folder": "data/china_featured",
	
	"lambda features": 0.99,

	"batch size": 4096,

	"statistics":
		{
			"statistics": "auxiliary/features_primary.npy",
			"warmup": 1000000,
			"split step": 5
		},

	"model":
		{
			"wing size": 3,
			
			"dropout rate": 0.1,
			"use common": false,
			"multiplier common": 20,
			"layers common": 3,

			"multiplier each": 10,
			"layers each": 3,

			"use batch normalization": true,

			"eviction lr": 1e-5,
			"admission lr": 1e-5

		},
	"testing":
		{
		"algorithms": ["AL-GDSF", "AL-LRU", "AL-Oracle", "SH-GDSF", "SH-LRU", "SH-Oracle", "ML-ML-RNG", "ML-ML-DET"],
		"period": 360,
		"requests max": 50000000,
		"reset": true,
		"alpha": 1
		},
	"training":
		{
			"target": "ML-LRU",

			"session configuration":
					{
						"collect discounted": false,
						"randomness change": false,
						"point of change": 100000,
						"gamma": 0.01,
						"alpha": 1
					},

			"requests max": 5000000,
			"samples": 150,


			"percentile admission": 95,
			"percentile eviction": 95,

			"epochs": 1,
			"warmup": 100000,
			"runs": 6,
			"repetitions": 1,
			"drop": true,
			
			"overlap": 0,
			"period": 200000,

			"refresh policy": "monotonic",
			"refresh value": 8,

			"algorithms": ["AL-GDSF", "AL-LRU", "AL-Oracle", "SH-GDSF", "SH-LRU", "SH-Oracle"],

			"iterative": false,
			"start iteration": "E",

			"IP:train admission": true,
			"IP:train eviction": true,

			"dump sessions": true,
			"dump limit": 200,
			"dump percentile": 75
		}

}
