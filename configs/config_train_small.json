{
	"model": "configs/config_model.json",

	"data folder": "data/china_featured",

	"cache size": 1024,

	"target": "ML-ML",

	"batch size": 4096,

	"session configuration":
	{
		"collect discounted": true,
		"change": true,
		"change mode": "random",
		"seed": 43,
		"initial gamma": 0.2,
		"gamma": 0.001,
		"alpha": 1
	},

	"samples": 150,
	"max samples": 75,

	"percentile admission": 90,
	"percentile eviction": 90,

	"epochs": 1,
	"warmup": 200000,
	"runs": 500,
	"refresh period": 2,
	"repetitions": 3,
	"drop": false,
	"store period": 3,
	"overlap": 100000,
	"period": 200000,

	"refresh policy": "static",
	"refresh value": 4,

	"algorithms": ["AL-GDSF", "AL-LRU", "AL-Oracle", "SH-GDSF", "SH-LRU", "SH-Oracle"],

	"iterative": true,
	"start iteration": "E",

	"IP:train admission": true,
	"IP:train eviction": true,

	"dump sessions": false,
	"dump limit": 200,
	"dump percentile": 75
}
